@model CSM.Web.Models.CustomerInfoViewModel
<div id="pnlCustomerInfo">
    <div class="row">
        <div class="col-md-4">
            <div class="form-group form-group-control">
                <div class="col-md-4">
                    <p class="form-control-static">
                        CIF ID:
                    </p>
                </div>
                <div class="col-md-8">
                    <p class="form-control input-sm static-view">
                        @Html.DisplayFor(model => model.CustomerNumberDisplay)
                    </p>
                </div>
            </div>
        </div>
        <div class="col-md-4">
            <div class="form-group form-group-control">
                <div class="col-md-4">
                    <p class="form-control-static">
                        ประเภทลูกค้า:
                    </p>
                </div>
                <div class="col-md-8">
                    <p class="form-control input-sm static-view">
                        @Html.DisplayFor(model => model.CustomerTypeDisplay)
                    </p>
                </div>
            </div>
        </div>
        <div class="col-md-4">
            <div class="form-group form-group-control">
                <div class="col-md-5">
                    <p class="form-control-static">
                        วันเกิด:
                    </p>
                </div>
                <div class="col-md-7">
                    <p class="form-control input-sm static-view">
                        @Html.DisplayFor(model => model.BirthDateDisplay)
                    </p>
                </div>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-md-4">
            <div class="form-group form-group-control">
                <div class="col-md-4">
                    <p class="form-control-static" title="เลขที่บัตรประชาชน/นิติบุุคคล/Passport/Employee No.">
                        รหัสบัตรประชาชน/นิติบุคคล:
                    </p>
                </div>
                <div class="col-md-8">
                    <p class="form-control input-sm static-view">
                        @Html.DisplayFor(model => model.CardNo)
                    </p>
                </div>
            </div>
        </div>
        <div class="col-md-4">
            <div class="form-group form-group-control">
                <div class="col-md-4">
                    <p class="form-control-static">
                        สัญญชาติ:
                    </p>
                </div>
                <div class="col-md-8">
                    <p class="form-control input-sm static-view">
                        @Html.DisplayFor(model => model.CountryOfCitizenship)
                    </p>
                </div>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-md-4">
            <div class="form-group form-group-control">
                <div class="col-md-4">
                    <p class="form-control-static">
                        คำนำหน้าภาษาไทย:
                    </p>
                </div>
                <div class="col-md-8">
                    <p class="form-control input-sm static-view">
                        @Html.DisplayFor(model => model.TitleThai.TitleName)
                    </p>
                </div>
            </div>
        </div>
        <div class="col-md-4">
            <div class="form-group form-group-control">
                <div class="col-md-4">
                    <p class="form-control-static">
                        ชื่อลูกค้า:
                    </p>
                </div>
                <div class="col-md-8">
                    <p class="form-control input-sm static-view">
                        @Html.DisplayFor(model => model.FirstNameThai)
                    </p>
                </div>
            </div>
        </div>
        <div class="col-md-4">
            <div class="form-group form-group-control">
                <div class="col-md-5">
                    <p class="form-control-static">
                        นามสกุลลูกค้า:
                    </p>
                </div>
                <div class="col-md-7">
                    <p class="form-control input-sm static-view">
                        @Html.DisplayFor(model => model.LastNameThai)
                    </p>
                </div>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-md-4">
            <div class="form-group form-group-control">
                <div class="col-md-4">
                    <p class="form-control-static">
                        Title:
                    </p>
                </div>
                <div class="col-md-8">
                    <p class="form-control input-sm static-view">
                        @Html.DisplayFor(model => model.TitleEnglish.TitleName)
                    </p>
                </div>
            </div>
        </div>
        <div class="col-md-4">
            <div class="form-group form-group-control">
                <div class="col-md-4">
                    <p class="form-control-static">
                        First Name:
                    </p>
                </div>
                <div class="col-md-8">
                    <p class="form-control input-sm static-view">
                        @Html.DisplayFor(model => model.FirstNameEnglish)
                    </p>
                </div>
            </div>
        </div>
        <div class="col-md-4">
            <div class="form-group form-group-control">
                <div class="col-md-5">
                    <p class="form-control-static">
                        Last Name:
                    </p>
                </div>
                <div class="col-md-7">
                    <p class="form-control input-sm static-view">
                        @Html.DisplayFor(model => model.LastNameEnglish)
                    </p>
                </div>
            </div>
        </div>
    </div>
</div>